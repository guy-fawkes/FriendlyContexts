{
    "name": "guy-fawkes/friendly-contexts",
    "description": "Some BEHAT contexts",
    "autoload": {
        "psr-0": { "Knp\\FriendlyContexts": "src/"}
    },
    "require": {
        "php": ">=7.0,<7.5",
        "behat/behat": "~3.0",
        "symfony/property-access": "~3.0",
        "doctrine/inflector": "~1.0",
        "behat/mink-extension": "~2.0",
        "fzaninotto/faker": "~1.4",
        "nelmio/alice": "~2.0 || ~3.0",
        "guzzle/guzzle": "~3.7"
    },
    "require-dev": {
        "phpspec/phpspec": "~3.4 || ~4.0 || ~6.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "~2.5",
        "symfony/symfony": "~3.0",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "~2.1",
        "bossa/phpspec2-expect": "~2.3 || ~3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "KnpLabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Pedrotroller",
            "homepage": "https://github.com/PedroTroller"
        },
        {
            "name": "Dan Kempster",
            "homepage": "https://github.com/dankempster"
        }
    ],
    "replace": {
        "knplabs/friendly-contexts": "self.version"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
